	snrtos_ipcu10: snrtos1@0 {
		compatible = "socionext,ipcu-device";
		dst-unit = <1>;
		dst-channel = <0>;
		interrupts = <0 38 4>;
		direction = "recv";
		reg = <0x1c252000 0x1000>, <GET_IPCU_SYNC_ADDR_2GB 0x32>; // IPCU sync top addr
