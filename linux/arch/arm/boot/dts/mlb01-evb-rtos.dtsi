	};

	snrtos_ipcu14: snrtos1@4 {
		compatible = "socionext,ipcu-device";
		dst-unit = <1>;
		dst-channel = <4>;
		src-int-ch = <12>;
		dst-int-ch = <5>;
		interrupts = <0 43 4>;
